
package pkg1070;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        int um;
        
        Scanner scan = new Scanner(System.in);
        
        System.out.println("Digite um valor inteiro");
        um = scan.nextInt();
        for ( int i = um ; i < um + 12 ; i ++ ) {
            if (i %  2  ==  1 ) {
            System.out.println("VALORES:"+um);
        }
  
    }
    
 }
}